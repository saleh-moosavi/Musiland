{
  "kind": "collectionType",
  "collectionName": "songs",
  "info": {
    "singularName": "song",
    "pluralName": "songs",
    "displayName": "Song"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "comments": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::comment.comment"
    },
    "likes": {
      "type": "biginteger",
      "default": "0"
    },
    "lyric": {
      "type": "text"
    },
    "album": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::album.album",
      "inversedBy": "songs"
    },
    "genres": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::genre.genre",
      "mappedBy": "songs"
    },
    "playlists": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::playlist.playlist",
      "mappedBy": "songs"
    },
    "singer": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::singer.singer"
    },
    "audioUrl": {
      "type": "string"
    },
    "coverUrl": {
      "type": "string"
    }
  }
}
