{
  "kind": "collectionType",
  "collectionName": "songs",
  "info": {
    "singularName": "song",
    "pluralName": "songs",
    "displayName": "Song"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "comments": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::comment.comment"
    },
    "likes": {
      "type": "biginteger"
    },
    "cover": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "audio": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "lyric": {
      "type": "text"
    },
    "publish_time": {
      "type": "datetime"
    },
    "album": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::album.album",
      "inversedBy": "songs"
    },
    "genres": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::genre.genre",
      "mappedBy": "songs"
    },
    "playlists": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::playlist.playlist",
      "mappedBy": "songs"
    },
    "singer": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::singer.singer"
    }
  }
}
